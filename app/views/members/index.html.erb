<p id="notice"><%= notice %></p>

<h1>Members</h1>

<table>
  <thead>
    <tr>
      <th>Last name</th>
      <th>First name</th>
      <th>Middle initial</th>
      <th>Id number</th>
      <th>Date of birth</th>
      <th>Gender</th>
      <th>Home number</th>
      <th>Cell number</th>
      <th>Address</th>
      <th>Alternate address</th>
      <th>Date of enrollment</th>
      <th>Date of disenrollment</th>
      <th>Line of business</th>
      <th>Primary language</th>
      <th>Interpreter needed</th>
      <th>Ethnicity</th>
      <th>Marital status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.last_name %></td>
        <td><%= member.first_name %></td>
        <td><%= member.middle_initial %></td>
        <td><%= member.id_number %></td>
        <td><%= member.date_of_birth %></td>
        <td><%= member.gender %></td>
        <td><%= member.home_number %></td>
        <td><%= member.cell_number %></td>
        <td><%= member.address %></td>
        <td><%= member.alternate_address %></td>
        <td><%= member.date_of_enrollment %></td>
        <td><%= member.date_of_disenrollment %></td>
        <td><%= member.line_of_business %></td>
        <td><%= member.primary_language %></td>
        <td><%= member.interpreter_needed %></td>
        <td><%= member.ethnicity %></td>
        <td><%= member.marital_status %></td>
        <td><%= link_to 'Show', member %></td>
        <td><%= link_to 'Edit', edit_member_path(member) %></td>
        <td><%= link_to 'Destroy', member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Member', new_member_path %>

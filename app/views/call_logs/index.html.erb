<p id="notice"><%= notice %></p>

<h1>Call Logs</h1>

<table>
  <thead>
    <tr>
      <th>Comments</th>
      <th>Member</th>
      <th>Time of call</th>
      <th>User</th>
      <th>Result of call</th>
      <th>Pcp provider call</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @call_logs.each do |call_log| %>
      <tr>
        <td><%= call_log.comments %></td>
        <td><%= call_log.member %></td>
        <td><%= call_log.time_of_call %></td>
        <td><%= call_log.user %></td>
        <td><%= call_log.result_of_call %></td>
        <td><%= call_log.pcp_provider_call %></td>
        <td><%= link_to 'Show', call_log %></td>
        <td><%= link_to 'Edit', edit_call_log_path(call_log) %></td>
        <td><%= link_to 'Destroy', call_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Call Log', new_call_log_path %>
